{
  "env": {
      "browser": true,
      "node": true
  },
 "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
        "jsx": true
    }
  },
  "rules": {
    "semi": [2, "always"],
    "indent": [2, 2],
    "quotes": [2, "single"],
    "arrow-parens": [2, "always"]
  }
}
